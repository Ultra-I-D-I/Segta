	<queryString>
		<![CDATA[SELECT count(*), s.nombre as nlote ,l.lote as contenedor
FROM tambor t 
LEFT JOIN sector s on t.idSector=s.idSector
LEFT JOIN lote l on t.idLote=l.idLote
WHERE  t.estado!='despachado'
GROUP BY nlote, contenedor]]>
	</queryString>
